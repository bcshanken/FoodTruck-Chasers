<body>
  <!-- Start Top Bar -->
  <div class="top-bar">
    <div class="top-bar-left">
      <ul class="menu">
        <li class="menu-text">Food Truck Chasers</li>
        <li><a href="#">One</a></li>
        <li><a href="#">Two</a></li>
        <li><a href="#">Three</a></li>
      </ul>
    </div>
  </div>
  <!-- End Top Bar -->
  <div class="callout medium red">
    <div class="row column text-center">
      <h2>Here are the trucks in your city.</h2>
    </div>
  </div>

  <div class="row small-10 medium-8 columns box">
    {{#each food_trucks}}
    <ul>
      <li>Food Truck: <b>{{truck_name}}</b></li>
      <li>Location: <b>{{city}}</b></li>
      <li>Today's Special!: <b>{{promo_message}}</b></li>
      <li>fFood Category: <b>{{category}}</b></li>
    </ul>
    {{/each}}
    <button type="button" class="button alert">edit</button>
    <button type="button" class="button alert">delete</button>
  </div>

  <script src="" async defer></script>
</body>

</html>